{
  "name": "apps-cli-app",
  "$schema": "../../../node_modules/nx/schemas/project-schema.json",
  "projectType": "application",
  "sourceRoot": "src",
  "tags": [
    "type:app",
    "scope:cli"
  ],
  "targets": {
    "build": {
      "executor": "@nrwl/gradle:run",
      "options": {
        "gradleTask": ":apps:cli-app:build"
      },
      "outputs": [
        "{workspaceRoot}/build"
      ]
    },
    "test": {
      "executor": "@nrwl/gradle:run",
      "options": {
        "gradleTask": ":apps:cli-app:test"
      }
    },
    "run": {
      "executor": "@nrwl/gradle:run",
      "options": {
        "gradleTask": ":apps:cli-app:run"
      }
    },
    "showInfo": {
      "executor": "@nrwl/gradle:run",
      "options": {
        "gradleTask": ":apps:cli-app:showInfo"
      }
    }
  }
}
