{
  "version": 2,
  "projectType": "library",
  "extends": "nx/presets/gradle.json",
  "plugins": [],
  "tasksRunnerOptions": {
    "default": {
      "runner": "@nrwl/gradle-runner",
      "options": {
        "cacheableOperations": ["build", "test", "compile"]
      }
    }
  },
  "projects": {
    "libs-core-utils": {
      "projectType": "library",
      "sourceRoot": "libs/core-utils/src",
      "targets": {
        "build": {
          "executor": "@nrwl/gradle:run",
          "options": {
            "gradleTask": ":libs:core-utils:build"
          },
          "outputs": ["{projectRoot}/build"]
        },
        "test": {
          "executor": "@nrwl/gradle:run",
          "options": {
            "gradleTask": ":libs:core-utils:test"
          }
        }
      }
    },
    "libs-data-models": {
      "projectType": "library",
      "sourceRoot": "libs/data-models/src",
      "targets": {
        "build": {
          "executor": "@nrwl/gradle:run",
          "options": {
            "gradleTask": ":libs:data-models:build"
          },
          "outputs": ["{projectRoot}/build"]
        },
        "test": {
          "executor": "@nrwl/gradle:run",
          "options": {
            "gradleTask": ":libs:data-models:test"
          }
        }
      }
    },
    "apps-service-app": {
      "projectType": "application",
      "sourceRoot": "apps/service-app/src",
      "targets": {
        "build": {
          "executor": "@nrwl/gradle:run",
          "options": {
            "gradleTask": ":apps:service-app:build"
          },
          "outputs": ["{projectRoot}/build"]
        },
        "test": {
          "executor": "@nrwl/gradle:run",
          "options": {
            "gradleTask": ":apps:service-app:test"
          }
        },
        "run": {
          "executor": "@nrwl/gradle:run",
          "options": {
            "gradleTask": ":apps:service-app:run"
          }
        }
      }
    },
    "apps-cli-app": {
      "projectType": "application",
      "sourceRoot": "apps/cli-app/src",
      "targets": {
        "build": {
          "executor": "@nrwl/gradle:run",
          "options": {
            "gradleTask": ":apps:cli-app:build"
          },
          "outputs": ["{projectRoot}/build"]
        },
        "test": {
          "executor": "@nrwl/gradle:run",
          "options": {
            "gradleTask": ":apps:cli-app:test"
          }
        },
        "run": {
          "executor": "@nrwl/gradle:run",
          "options": {
            "gradleTask": ":apps:cli-app:run"
          }
        }
      }
    }
  }
}
