{
  "$schema": "./node_modules/nx/schemas/nx-schema.json",
  "version": 2,
  "cli": {
    "analytics": false,
    "packageManager": "npm"
  },
  "defaultProject": "lesson16-nx-gradle",
  "tasksRunnerOptions": {
    "default": {
      "runner": "nx/tasks-runners/default",
      "options": {
        "cacheableOperations": ["build", "test", "compile", "check"],
        "executionTimeout": 300
      }
    }
  },
  "targetDefaults": {
    "build": {
      "cache": true
    },
    "test": {
      "cache": true
    }
  }
}
